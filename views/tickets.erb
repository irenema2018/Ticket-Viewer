

<% @tickets.each do |ticket| %>
  <div>
    <a href="/ticket/<%= ticket['id'] %>">
      <%= ticket['id'] %> 
      <%= ticket['subject'] %>
      <%= ticket['status'] %>
      <%= ticket['type'] %>
      <%= @users[ticket['requester_id']] %>
      <%= @users[ticket['assignee_id']] %>

    </a>
  </div>
<% end %>

<% if session[:prev_page_url] %>
  <a href="/tickets/prev_page">Prev</a>
<% end %>

<% if session[:next_page_url] %>
  <a href="/tickets/next_page">Next</a>
<% end %>